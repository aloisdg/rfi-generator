<wfs:Transaction xmlns:wfs="http://www.opengis.net/wfs"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:gml="http://www.opengis.net/gml"
    xmlns:ogc="http://www.opengis.net/ogc"
    service="WFS"
    version="1.1.0"
    xsi:schemaLocation="http://www.opengis.net/wfs http://schemas.opengis.net/wfs/1.1.0/wfs.xsd http://localhost:8080 http://localhost:8080/geoserver/wfs/DescribeFeatureType?version=1.1.0&amp;typename=rfi:rfi">
  <wfs:Delete typeName="rfi:rfi">
    <ogc:Filter>
        <ogc:PropertyIsEqualTo>
            <ogc:PropertyName>rfi_id</ogc:PropertyName>
            <ogc:Literal>%d</ogc:Literal>
        </ogc:PropertyIsEqualTo>
    </ogc:Filter>
  </wfs:Delete>
</wfs:Transaction>
